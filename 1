Offer Sprd = 
VAR CurrentTradeDate = [tradedate]
VAR CurrentTradeTime = [tradetime]

VAR MaxYield =
    CALCULATE(
        MAX('YourTable'[yield]),
        FILTER(
            'YourTable',
            'YourTable'[tradedate] = CurrentTradeDate &&
            'YourTable'[tradetime] = CurrentTradeTime &&
            NOT(ISBLANK('YourTable'[yield]))
        )
    )

VAR MinYield =
    CALCULATE(
        MIN('YourTable'[yield]),
        FILTER(
            'YourTable',
            'YourTable'[tradedate] = CurrentTradeDate &&
            'YourTable'[tradetime] = CurrentTradeTime &&
            NOT(ISBLANK('YourTable'[yield]))
        )
    )

RETURN
    ABS(MaxYield - MinYield)
