if 
    (
        not Text.IsNullOrEmpty([PAYACCT]) and not Text.IsNullOrEmpty([RECACCT]) and
        (
            (Text.Contains([PAYACCT], "BOF", Comparer.OrdinalIgnoreCase) and Text.Contains([RECACCT], "EUROCLEAR", Comparer.OrdinalIgnoreCase)) or 
            (Text.Contains([PAYACCT], "EUROCLEAR", Comparer.OrdinalIgnoreCase) and Text.Contains([RECACCT], "BOF", Comparer.OrdinalIgnoreCase))
        )
    ) then "BOF <-> EC"
else if 
    (
        not Text.IsNullOrEmpty([PAYACCT]) and not Text.IsNullOrEmpty([RECACCT]) and
        (
            (Text.Contains([PAYACCT], "DBB", Comparer.OrdinalIgnoreCase) and Text.Contains([RECACCT], "EUROCLEAR", Comparer.OrdinalIgnoreCase)) or 
            (Text.Contains([PAYACCT], "EUROCLEAR", Comparer.OrdinalIgnoreCase) and Text.Contains([RECACCT], "DBB", Comparer.OrdinalIgnoreCase))
        )
    ) then "DBB <-> EC"
/* Repeat for other conditions */
else "Other"

